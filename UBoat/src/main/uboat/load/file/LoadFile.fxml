<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>


<HBox spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.uboat.load.file.LoadFileController">
   <children>
      <Button fx:id="loadFileButton" minWidth="-Infinity" mnemonicParsing="false" onAction="#openFileButtonAction" text="Load File Button" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <TextField fx:id="currentLoadedFilePathTextField" editable="false" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </TextField>
   </children>
</HBox>
