<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.uboat.tabs.screen.contest.game.setting.setter.details.fill.DetailsFillController">
   <children>
      <VBox alignment="TOP_CENTER" spacing="15.0">
         <children>
            <HBox spacing="15.0">
               <children>
                  <Label prefWidth="110.0" text="Message To Encode:" />
                  <TextField fx:id="messageToEncodeTextField" />
               </children>
            </HBox>
            <Button fx:id="sendDataButton" alignment="TOP_LEFT" mnemonicParsing="false" text="Send Data" />
            <fx:include fx:id="dictionary" source="dictionary/Dictionary.fxml" />
         </children>
      </VBox>
   </children>
</HBox>
