<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.uboat.tabs.screen.contest.game.setting.setter.user.updater.UserUpdaterController">
   <children>
      <VBox alignment="TOP_CENTER" spacing="15.0">
         <children>
            <HBox spacing="15.0">
               <children>
                  <Label text="The Message You Entered:" />
                  <Label fx:id="originalMessageLabel" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="The Message To Encode (Without Exclude Chars):" />
                  <Label fx:id="messageToEncodeLabel" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="The Message To Decode:" />
                  <Label fx:id="messageToDecodeLabel" />
               </children>
            </HBox>
            <VBox alignment="TOP_CENTER" spacing="15.0">
               <children>
                  <Button fx:id="readyButton" mnemonicParsing="false" text="Ready" />
                  <HBox alignment="TOP_CENTER" spacing="15.0">
                     <children>
                        <Label text="Is Ready:" />
                        <Label fx:id="isReadyValueLabel" text="Not Yet!" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</HBox>
