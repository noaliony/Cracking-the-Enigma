<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.agent.operation.screen.data.contest.details.ContestDetailsController">
   <children>
      <Label text="Contest Details" underline="true" />
      <HBox spacing="15.0">
         <children>
            <Label text="Battlefield Name:" />
            <Label fx:id="battlefieldNameValueLabel" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="UBoat Name:" />
            <Label fx:id="uBoatNameValueLabel" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="Game Status:" />
            <Label fx:id="gameStatusValueLabel" text="idle" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="Game Level:" />
            <Label fx:id="gameLevelValueLabel" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="Allies Count:" />
            <Label fx:id="registeredAlliesCountValueLabel" text="0" />
            <Label text="/" />
            <Label fx:id="totalAlliesCountValueLabel" />
         </children>
      </HBox>
   </children>
</VBox>
