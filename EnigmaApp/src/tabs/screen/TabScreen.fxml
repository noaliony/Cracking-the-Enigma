         fx:controller="tabs.screen.TabScreenController">
    <Tab text="Machine">
        <fx:include fx:id="machine" source="../machine/Machine.fxml"/>
    </Tab>
    <Tab fx:id="encryptDecryptTab" text="Encrypt \ Decrypt">
        <fx:include fx:id="encryptDecrypt" source="../encryptdecrypt/EncryptDecrypt.fxml"/>
    </Tab>
    <Tab fx:id="bruteForceTab" text="Brute force">
        <fx:include fx:id="bruteForce" source="../bruteforce/BruteForce.fxml"/>
    </Tab>
</TabPane>
