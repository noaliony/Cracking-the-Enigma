<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabs.bruteforce.BruteForceController">
   <content>
      <VBox spacing="10.0">
         <children>
            <fx:include source="../../title/CurrentCodeConfigurationTitle.fxml" />
            <fx:include fx:id="currentConfiguration" source="../machine/current/configuration/CurrentConfiguration.fxml" />
            <Button fx:id="resetConfigurationButton" alignment="CENTER" mnemonicParsing="false" text="Reset Configuration" />
            <HBox alignment="TOP_CENTER" spacing="15.0">
               <children>
                  <Button fx:id="gameSettingSetterButton" mnemonicParsing="false" onAction="#gameSettingSetterButtonAction" text="Game Setting Setter" />
                  <Button fx:id="progressDisplayButton" mnemonicParsing="false" onAction="#progressDisplayButtonAction" text="Progress Display" />
                  <Button fx:id="resultDisplayButton" mnemonicParsing="false" onAction="#resultDisplayButtonAction" text="Result Display" />
               </children>
            </HBox>
            <StackPane fx:id="bruteForcePanel">
               <children>
                  <fx:include fx:id="gameSettingSetter" source="game/setting/setter/GameSettingSetter.fxml" />
                  <fx:include fx:id="progressDisplay" source="progress/display/ProgressDisplay.fxml" />
                  <fx:include fx:id="resultDisplay" source="result/display/ResultDisplay.fxml" />
               </children>
            </StackPane>
         </children>
      </VBox>
   </content>
</ScrollPane>
