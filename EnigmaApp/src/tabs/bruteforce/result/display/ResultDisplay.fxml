<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabs.bruteforce.result.display.ResultDisplayController">
   <content>
      <VBox alignment="TOP_CENTER" spacing="15.0">
         <children>
            <HBox spacing="15.0">
               <children>
                  <Label text="Game Status:" />
                  <Label fx:id="gameStatusValueLabel" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="Winner Configuration:" />
                  <Label fx:id="winnerAgentValueLabel" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="Game Duration Time:" />
                  <Label fx:id="gameDurationTimeValueLabel" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="Average Duration Time To Each Task:" />
                  <Label fx:id="averageDurationTimeToEachTaskValueLabel" />
               </children>
            </HBox>
            <Button fx:id="playAgainButton" alignment="TOP_CENTER" mnemonicParsing="false" text="Play Again" />
         </children>
      </VBox>
   </content>
</ScrollPane>
