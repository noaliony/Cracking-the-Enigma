<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabs.bruteforce.game.setting.setter.user.updater.UserUpdaterController">
   <children>
      <HBox spacing="15.0">
         <children>
            <Label text="The Message You Entered:" />
            <Label fx:id="originalMessageLabel" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="The Message To Encode (Without Exclude Chars):" />
            <Label fx:id="messageToEncodeLabel" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="The Message To Decode:" />
            <Label fx:id="messageToDecodeLabel" />
         </children>
      </HBox>
      <HBox spacing="15.0">
         <children>
            <Label text="Tasks Count:" />
            <Label fx:id="tasksCountValueLabel" />
         </children>
      </HBox>
      <Button fx:id="startButton" mnemonicParsing="false" text="Start" />
   </children>
</VBox>
