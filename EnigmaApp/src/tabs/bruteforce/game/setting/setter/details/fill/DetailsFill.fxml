<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabs.bruteforce.game.setting.setter.details.fill.DetailsFillController">
   <children>
      <VBox spacing="15.0">
         <children>
            <HBox spacing="15.0">
               <children>
                  <Label text="Agents Count:" />
                  <Slider fx:id="agentsCountSlider" min="2.0" />
                  <Label fx:id="agentsCountLabel" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="Game Level:" />
                  <ComboBox fx:id="gameLevelComboBox" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="Task Size:" />
                  <TextField fx:id="taskSizeValueTextField" />
               </children>
            </HBox>
            <HBox spacing="15.0">
               <children>
                  <Label text="Message To Encode:" />
                  <TextField fx:id="messageToEncodeTextField" />
               </children>
            </HBox>
            <Button fx:id="sendDataButton" alignment="TOP_LEFT" mnemonicParsing="false" text="Send Data" />
         </children>
      </VBox>
      <fx:include fx:id="dictionary" source="dictionary/Dictionary.fxml" />
   </children>
</HBox>
