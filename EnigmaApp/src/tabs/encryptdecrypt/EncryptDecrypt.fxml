<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToWidth="true" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tabs.encryptdecrypt.EncryptDecryptController">
   <content>
      <VBox spacing="15.0">
         <children>
            <ScrollPane fitToHeight="true" fitToWidth="true" style="-fx-background-color: transparent;">
               <content>
                  <VBox spacing="10.0">
                     <children>
                        <fx:include source="../../title/CurrentCodeConfigurationTitle.fxml" />
                        <fx:include fx:id="currentConfiguration" source="../machine/current/configuration/CurrentConfiguration.fxml" />
                        <fx:include fx:id="inputProcessor" source="input/processor/InputProcessor.fxml" />
                     <HBox alignment="CENTER" spacing="10.0">
                        <children>
                           <Button fx:id="resetConfigurationButton" alignment="CENTER" mnemonicParsing="false" text="Reset Configuration" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" />
                        </children>
                     </HBox>
                        <fx:include fx:id="machineHistoryView" source="history/MachineHistoryViewController.fxml" />
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </content>
</ScrollPane>
