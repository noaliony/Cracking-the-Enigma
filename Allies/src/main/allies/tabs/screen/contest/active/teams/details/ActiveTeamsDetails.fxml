   <children>
      <Label text="Active Teams Details:" underline="true" />
      <TableView fx:id="teamsDetailsTableView" prefHeight="200.0">
        <columns>
          <TableColumn fx:id="userNameCol" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="140.0" text="Ally Name" />
          <TableColumn fx:id="agentsCountCol" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="140.0" text="Agents Count" />
            <TableColumn fx:id="taskSizeCol" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="140.0" text="Task Size" />
        </columns>
      </TableView>
   </children>
</VBox>
